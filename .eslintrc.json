{
  "env": {
    "es2020": true
  },
  "extends": [
    "react-app",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "import/no-webpack-loader-syntax": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "jsx-a11y/label-has-for": [
      2,
      {
        "components": [],
        "required": {
          "every": [
            "nesting",
            "id"
          ]
        },
        "allowChildren": true
      }
    ]
  }
}
